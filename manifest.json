{
  "manifest_version": 2,
  "name": "Kayee's Awesome Extension",
  "description": "",
  "version": "1.0",
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
    }
  ]
}